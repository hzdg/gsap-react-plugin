# editorconfig.org
root = true

[*]
indent_style = tab
end_of_line = lf
charset = utf-8
trim_trailing_whitespace = true
insert_final_newline = true

[*.{md,markdown}]
indent_style = space
indent_size = 4
trim_trailing_whitespace = false

[*.{coffee,litcoffee}]
indent_style = space
indent_size = 2

[*.js]
indent_style = space
indent_size = 4
